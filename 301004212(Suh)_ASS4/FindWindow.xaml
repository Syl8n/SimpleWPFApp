<Window x:Class="_301004212_Suh__ASS4.FindWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_301004212_Suh__ASS4"
        mc:Ignorable="d"
        Title="Search Customer" Height="120" Width="220" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Input ID:" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Margin="10,31,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="57" Height="20"
                 Text="{Binding SearchId}"/>
        <Button Content="Search" Margin="78,31,0,0"
                Click="Search_Click" Height="20" VerticalAlignment="Top" HorizontalAlignment="Left" Width="112"/>

    </Grid>
</Window>
